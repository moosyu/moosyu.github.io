---
import Emojis from "./Emojis.astro";
const { info } = Astro.props;
---
<details style="display: flex; align-items: center; cursor: default;">
    <summary style="cursor: pointer; list-style: none; filter: drop-shadow(4px 3px 3px rgba(0, 0, 0, 1));"><Emojis className="nav-emojis" name="question-mark" /></summary>
    <span set:html={info}></span>
</details>